<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    		    android:id="@+id/layout_propertyInternalAminities"
			    android:layout_width="fill_parent"
			    android:layout_height="wrap_content"
			    android:orientation="vertical"
			    android:visibility="gone">
	        
    
                   <TextView 
			           android:layout_width="wrap_content"
			           android:layout_height="wrap_content"
			           android:text="Internal Amenities"
			           android:layout_margin="10dp"
			           android:textAppearance="?android:attr/textAppearanceMedium"
			           android:textStyle="bold"
			           android:textColor="@android:color/black"
			           android:layout_alignParentLeft="true"/>
                   
		             <RelativeLayout
		                android:layout_width="fill_parent"
					    android:layout_height="wrap_content">
		                
		                 <TextView 
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:text="Furnished"
				           android:layout_margin="10dp"
				           android:textAppearance="?android:attr/textAppearanceSmall"
				           android:textStyle="bold"
				           android:textColor="@android:color/black"
				           android:layout_alignParentLeft="true"/>
		                 
		                 <ImageView 
		                   android:id="@+id/internalAmenities_Furnished"  
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:layout_margin="10dp"
				           android:layout_alignParentRight="true"
				           />
		                 
		             </RelativeLayout>
		             
		             <RelativeLayout
		                android:layout_width="fill_parent"
					    android:layout_height="wrap_content">
		                
		                 <TextView 
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:text="Inverter"
				           android:layout_margin="10dp"
				           android:textAppearance="?android:attr/textAppearanceSmall"
				           android:textStyle="bold"
				           android:textColor="@android:color/black"
				           android:layout_alignParentLeft="true"/>
		                 
		                 <ImageView 
		                   android:id="@+id/internalAmenities_Inverter"  
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:layout_margin="10dp"
				           android:layout_alignParentRight="true"
				           />
		                 
		             </RelativeLayout>
		             
		             <RelativeLayout
		                android:layout_width="fill_parent"
					    android:layout_height="wrap_content">
		                
		                 <TextView 
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:text="PipedGas"
				           android:layout_margin="10dp"
				           android:textAppearance="?android:attr/textAppearanceSmall"
				           android:textStyle="bold"
				           android:textColor="@android:color/black"
				           android:layout_alignParentLeft="true"/>
		                 
		                 <ImageView 
		                   android:id="@+id/internalAmenities_PipedGas"  
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:layout_margin="10dp"
				           android:layout_alignParentRight="true"
				           />
		                 
		                 
		             </RelativeLayout>
		             
		             <RelativeLayout
		                android:layout_width="fill_parent"
					    android:layout_height="wrap_content">
		                
		                 <TextView 
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:text="GasLeakDetector"
				           android:layout_margin="10dp"
				           android:textAppearance="?android:attr/textAppearanceSmall"
				           android:textStyle="bold"
				           android:textColor="@android:color/black"
				           android:layout_alignParentLeft="true"/>
		                 
		                 <ImageView 
		                   android:id="@+id/internalAmenities_GasLeakDetector"  
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:layout_margin="10dp"
				           android:layout_alignParentRight="true"
				           />
		                 
		             </RelativeLayout>
		             
		             
		             <RelativeLayout
		                android:layout_width="fill_parent"
					    android:layout_height="wrap_content">
		                
		                 <TextView 
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:text="PanicButton"
				           android:layout_margin="10dp"
				           android:textAppearance="?android:attr/textAppearanceSmall"
				           android:textStyle="bold"
				           android:textColor="@android:color/black"
				           android:layout_alignParentLeft="true"/>
		                 
		                 <ImageView 
		                   android:id="@+id/internalAmenities_PanicButton"  
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:layout_margin="10dp"
				           android:layout_alignParentRight="true"
				           />
		                 
		             </RelativeLayout>
		             
		             <RelativeLayout
		                android:layout_width="fill_parent"
					    android:layout_height="wrap_content">
		                
		                 <TextView 
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:text="VideoDoorphone"
				           android:layout_margin="10dp"
				           android:textAppearance="?android:attr/textAppearanceSmall"
				           android:textStyle="bold"
				           android:textColor="@android:color/black"
				           android:layout_alignParentLeft="true"/>
		                 
		                <ImageView 
		                   android:id="@+id/internalAmenities_VideoDoorphone"  
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:layout_margin="10dp"
				           android:layout_alignParentRight="true"
				           />
		                 
		             </RelativeLayout>
		             
		             <RelativeLayout
		                android:layout_width="fill_parent"
					    android:layout_height="wrap_content">
		                
		                 <TextView 
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:text="Intercom"
				           android:layout_margin="10dp"
				           android:textAppearance="?android:attr/textAppearanceSmall"
				           android:textStyle="bold"
				           android:textColor="@android:color/black"
				           android:layout_alignParentLeft="true"/>
		                 
		                 <ImageView 
		                   android:id="@+id/internalAmenities_Intercom"  
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:layout_margin="10dp"
				           android:layout_alignParentRight="true"
				           />
		                 
		             </RelativeLayout>
		             
		             
		             <RelativeLayout
		                android:layout_width="fill_parent"
					    android:layout_height="wrap_content">
		                
		                 <TextView 
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:text="Pets"
				           android:layout_margin="10dp"
				           android:textAppearance="?android:attr/textAppearanceSmall"
				           android:textStyle="bold"
				           android:textColor="@android:color/black"
				           android:layout_alignParentLeft="true"/>
		                 
		                 <ImageView 
		                   android:id="@+id/internalAmenities_Pets"  
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:layout_margin="10dp"
				           android:layout_alignParentRight="true"
				           />
		                 
		                 
		             </RelativeLayout>
		             
		             <RelativeLayout
		                android:layout_width="fill_parent"
					    android:layout_height="wrap_content">
		                
		                 <TextView 
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:text="WaterStorage"
				           android:layout_margin="10dp"
				           android:textAppearance="?android:attr/textAppearanceSmall"
				           android:textStyle="bold"
				           android:textColor="@android:color/black"
				           android:layout_alignParentLeft="true"/>
		                
		                 <ImageView 
		                   android:id="@+id/internalAmenities_WaterStorage"  
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:layout_margin="10dp"
				           android:layout_alignParentRight="true"
				           />
		                 
		                 
		             </RelativeLayout>
		             
		             <RelativeLayout
		                android:layout_width="fill_parent"
					    android:layout_height="wrap_content">
		                
		                 <ImageView
		                   android:id="@+id/onBackInternalAmenitiesPage"   
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:layout_margin="20dp"
				           android:layout_alignParentLeft="true"
				           android:background="@drawable/back"/>
		                
		                 <ImageView 
		                   android:id="@+id/onNextInternalAmenitiesPage"  
				           android:layout_width="wrap_content"
				           android:layout_height="wrap_content"
				           android:layout_margin="20dp"
				           android:layout_alignParentRight="true"
				           android:src="@drawable/back"/>
		                 
		                 
		             </RelativeLayout>
		             
		        
        </LinearLayout>
	       
	        